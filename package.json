{
  "name": "ba-online-toolbox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "format": "prettier --config .prettierrc.json -uw ./src/ *.md",
    "check": "vue-tsc --noEmit; eslint --config ./.eslintrc.json ./src/ --ext .js,.ts,.vue",
    "build:json": "eslint --config ./.eslintrc.json ./public/ && node ./CICD/build_json.js",
    "build": "pnpm format; pnpm build:json && vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "9.12.0",
    "axios": "1.3.0",
    "crypto-js": "4.1.1",
    "file-saver": "2.0.5",
    "html-to-image": "1.11.11",
    "js-yaml": "4.1.0",
    "pinia": "2.0.29",
    "pinia-plugin-persistedstate": "3.0.2",
    "vue": "3.2.45",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/crypto-js": "4.1.1",
    "@types/file-saver": "2.0.5",
    "@types/js-yaml": "4.0.5",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "@vitejs/plugin-legacy": "3.0.2",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/compiler-sfc": "3.x",
    "@vue/eslint-config-typescript": "11.0.2",
    "chalk": "5.2.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.9.0",
    "eslint-plugin-yaml": "0.5.0",
    "naive-ui": "2.34.3",
    "postcss": "8.4.21",
    "postcss-plugin-px2rem": "0.8.1",
    "postcss-preset-env": "8.0.1",
    "prettier": "2.8.3",
    "prettier-plugin-css-order": "1.3.0",
    "sass": "1.58.0",
    "terser": "5.16.2",
    "typescript": "4.9.5",
    "vfonts": "0.0.3",
    "vite": "4.0.4",
    "vue-eslint-parser": "9.1.0",
    "vue-tsc": "1.0.24"
  }
}
